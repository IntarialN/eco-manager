{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug requirements-service with Nx",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["nx", "serve", "requirements-service"],
      "env": {
        "NODE_OPTIONS": "--inspect=9229"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
